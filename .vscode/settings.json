// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.jest-cache": true,
    "**/.awcache": true,
    "**/.server": true,
    "**/node_modules": false,
    "**/coverage": false,
    "**/jest": false,
    "**/hybris": true,
    "**/build-tools": true,
    "**/c4c-integration": true,
    "**/hybris-ems": true,
    "**/hybris-sbg": true,
    "**/installer": true,
    "**/licenses": true,
    "**/hybris-Mobile-Apps-SDK": true,
    "**/*.log": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.jest-cache": true,
    "**/*.snap": true,
    "**/hybris": true,
    "**/.storybook": true,
    "**/build-tools": true,
    "**/c4c-integration": true,
    "**/hybris-ems": true,
    "**/hybris-sbg": true,
    "**/installer": true,
    "**/licenses": true,
    "**/hybris-Mobile-Apps-SDK": true,
    "**/*.log": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": true,
  "git.ignoreLimitWarning": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true
}
