{
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "setupFilesAfterEnv": ["@testing-library/jest-dom"],
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  },
  "modulePathIgnorePatterns": ["src/main.tsx"],
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.{js,jsx,tsx}"]
}
